{
  "name": "scanner",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node api/server.js && node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "start:dev": "NODE_ENV=development DEBUG=APP_NAME:* nodemon api/server.js",
    "db:create:migration": "sequelize migration:generate --name ",
    "db:migrate": "sequelize db:migrate",
    "db:rollback": "sequelize db:migrate:undo"
  },
  "dependencies": {
    "aws-sdk": "^2.343.0",
    "express": "^4.16.4",
    "helmet": "^3.9.0",
    "multer": "^1.4.1",
    "multer-s3": "^2.7.0",
    "pg": "^7.4.0",
    "pg-promise": "^7.3.1",
    "react": "16.3.1",
    "react-native": "0.55.4",
    "react-native-camera": "^1.3.1",
    "sequelize": "^4.40.0",
    "sequelize-cli": "^5.1.0"
  },
  "devDependencies": {
    "babel-jest": "23.0.1",
    "babel-preset-react-native": "4.0.0",
    "dotenv": "^6.1.0",
    "jest": "23.1.0",
    "nodemon": "^1.18.4",
    "react-test-renderer": "16.3.1"
  },
  "jest": {
    "preset": "react-native"
  },
  "rnpm": {
    "assets": [
      "./app/fonts"
    ]
  }
}
